  "dependencies": {
    "discord.js": "^14.6.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "dotenv": "^16.0.3",
    "jest": "^29.4.3"
  }
}
